{
    "name": "poptin/silverstripe-popups-forms",
    "description": "SilverStripe Pop ups, Exit Intent Popup, Email Pop Up, Inline Contact Form Builder - Poptin",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "poptin",
        "silverstripe",
        "silverstripe-popups-forms",
        "pop ups",
        "pop up",
        "popups",
        "pop up",
        "exit intent",
        "exit intent popup",
        "exit intent technology",
        "email popups",
        "exit pop up",
        "video popup",
        "forms",
        "form",
        "contact forms",
        "inline forms",
        "embedded forms"
    ],
    "homepage": "https://www.poptin.com/",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "guzzlehttp/guzzle": "^7.0",
        "silverstripe/cms": "^4.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Poptin\\Silverstripe\\": "src/",
            "Poptin\\Silverstripe\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}